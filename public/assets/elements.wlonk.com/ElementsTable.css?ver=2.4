div#elementsContainer {
	z-index: 1;
	position: relative;
	max-width: 100%;
}
div.keyBox {
	z-index: 2;
	position: absolute;
}
div#keyBox1 {
	top: 5%;
	left: 13%;
	width: 15%;
}
div#keyBox2 {
	top: 5%;
	left: 29%;
	width: 15%;
}
div#keyBox3 {
	top: 8%;
	left: 45%;
	width: 20%;
}
div.keyBox img {
	width: 100%;
	max-width: 100%; 
	height: auto;
	cursor: pointer;
}
div#popupBox1 {
	z-index: 3;
	display: none;
	position: absolute;
	top: 15%;
	left: 20%;
	width: 200px;
}
img#popupImg1 {
	width: 100%;
	max-width: 100%; 
	height: auto;
	border: 3px solid #999999;
	cursor: pointer;
}
div.auxBox {
	z-index: 2;
	position: absolute;
	font-size: smaller;
	text-align: center;
	padding: .1em .1em;
	border-radius: 1em;
	background-color: #EEEEEE;
}
div#auxBox1 {
	top: 0%;
	left: 0%;
	width: 5%;
}
div#auxBox2 {
	top: 0%;
	left: 6%;
	width: 5%;
}
div#auxBox3 {
	top: 0%;
	left: 94%;
	width: 6%;
	background-color: transparent;
}
div.speakerBox {
	z-index: 2;
	position: absolute;
}
div#speakerBox1 {
	top: 5%;
	left: 10%;
	width: 2%;
}
div.speakerBox img {
	width: 100%;
	max-width: 100%; 
	height: auto;
	cursor: pointer;
}